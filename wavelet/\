subplot(221)
plot(testy);
xlim([200 1024+200]);
title('Observed noisy signal');

subplot(222)
plot(decall(testy,K,'allp',1));
xlim([0 1024]);
title('Deconvolved with allpass');

z = wex(3,:);
subplot(223)
plot(z);
xlim([0 1024]);
title('Observed noisy signal');

subplot(224)
plot(decall(z,K,'allp',1));
xlim([0 1024]);
title('Deconvolved with allpass');
